{
    "name": "comphppuebla/dbal-fixtures",
    "description": "Fixtures managament with Doctrine DBAL",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "luis.montealegre@mandragora-web-systems.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/dbal": "~2.4",
        "symfony/console": "~2.4",
        "symfony/yaml": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "ptrofimov/xpmock": "~1.1",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.1"
    },
    "autoload": {
        "psr-0": {
            "ComPHPPuebla": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod u+x bin/cli"
        ],
        "post-update-cmd": [
            "chmod u+x bin/cli"
        ]
    }
}
